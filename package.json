{
  "name": "javascript-library-Boilerplate",
  "version": "0.1.0",
  "description": "Build your Javascript library without hassle",
  "main": "./src/index.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -wc",
    "prepublishOnly": "rollup -c",
    "test": "cross-env COVERAGE=true yarn karma start",
    "test:open": "cross-env COVERAGE=true yarn karma start --open",
    "docs": "node docs.js && yarn format:docs",
    "format:docs": "prettier --write '**/*.md'"
  },
  "keywords": [
    "boilerplate",
    "template",
    "javascript",
    "library"
  ],
  "author": "Adam Sherwood <adam.the.sherwood@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/theSherwood/javascript-library-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "ansi-colors": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^22.1.0",
    "esm": "^3.2.25",
    "faucet": "0.0.1",
    "fs-extra": "^9.0.0",
    "jsdoc-to-markdown": "^5.0.3",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-sauce-launcher": "^4.1.2",
    "karma-tap": "^4.2.0",
    "karma-tap-pretty-reporter": "^4.2.0",
    "minimist": "^1.2.5",
    "prettier": "^2.0.4",
    "rollup": "^2.4.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-gzip": "^2.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.3.0",
